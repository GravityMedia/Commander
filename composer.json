{
    "name": "gravitymedia/commander",
    "description": "Commander is a task manager/runner application for PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-pdo_sqlite": "*",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "monolog/monolog": "^1.21",
        "symfony/console": "^3.1",
        "symfony/process": "^3.1",
        "symfony/serializer": "^3.1",
        "zendframework/zend-servicemanager": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Commander\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\CommanderTest\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.*",
            "/box.json",
            "/build.xml",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    },
    "bin": [
        "bin/commander",
        "bin/commander.php"
    ]
}
