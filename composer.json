{
    "name": "gravitymedia/commander",
    "description": "Commander is a task manager/runner application for PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-pdo_sqlite": "*",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony/console": "^2.8",
        "symfony/process": "^2.8",
        "symfony/serializer": "^2.8",
        "zendframework/zend-servicemanager": "^3.1"
    },
    "require-dev": {
        "phing/phing": "^2.14",
        "phpunit/phpunit": "^5.3",
        "sami/sami": "^3.2",
        "scrutinizer/ocular": "^1.3",
        "symfony/finder": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Commander\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\CommanderTest\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.*",
            "/*.php",
            "/build.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    }
}
